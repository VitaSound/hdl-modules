#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5f90d6c76490 .scope module, "testbench" "testbench" 2 3;
 .timescale -6 -9;
P_0x5f90d6c9ac60 .param/l "TEST_DURATION" 0 2 5, +C4<00000000000011110100001001000000>;
P_0x5f90d6c9aca0 .param/l "WIDTH" 0 2 4, +C4<00000000000000000000000000100000>;
v0x5f90d6cc3e00_0 .var "adder_input", 31 0;
v0x5f90d6cc3ee0_0 .var "clk", 0 0;
v0x5f90d6cc3fb0_0 .var "reset", 0 0;
v0x5f90d6cc40b0_0 .net "signal_out", 31 0, v0x5f90d6cc3c70_0;  1 drivers
S_0x5f90d6c766f0 .scope module, "dut" "dds" 2 24, 3 1 0, S_0x5f90d6c76490;
 .timescale -6 -9;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 32 "adder";
    .port_info 3 /OUTPUT 32 "signal_out";
P_0x5f90d6cb1620 .param/l "WIDTH" 0 3 2, +C4<00000000000000000000000000100000>;
v0x5f90d6cb1720_0 .net "adder", 31 0, v0x5f90d6cc3e00_0;  1 drivers
v0x5f90d6cc3ae0_0 .net "clk", 0 0, v0x5f90d6cc3ee0_0;  1 drivers
v0x5f90d6cc3ba0_0 .net "reset", 0 0, v0x5f90d6cc3fb0_0;  1 drivers
v0x5f90d6cc3c70_0 .var "signal_out", 31 0;
E_0x5f90d6cb2690 .event posedge, v0x5f90d6cc3ae0_0;
    .scope S_0x5f90d6c766f0;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x5f90d6cc3c70_0, 0;
    %end;
    .thread T_0;
    .scope S_0x5f90d6c766f0;
T_1 ;
    %wait E_0x5f90d6cb2690;
    %load/vec4 v0x5f90d6cc3ba0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x5f90d6cc3c70_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5f90d6cc3c70_0;
    %load/vec4 v0x5f90d6cb1720_0;
    %add;
    %assign/vec4 v0x5f90d6cc3c70_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5f90d6c76490;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5f90d6cc3ee0_0, 0;
    %end;
    .thread T_2;
    .scope S_0x5f90d6c76490;
T_3 ;
    %delay 10000, 0;
    %load/vec4 v0x5f90d6cc3ee0_0;
    %inv;
    %assign/vec4 v0x5f90d6cc3ee0_0, 0;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5f90d6c76490;
T_4 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f90d6cc3fb0_0, 0, 1;
    %pushi/vec4 33333333, 0, 32;
    %assign/vec4 v0x5f90d6cc3e00_0, 0;
    %delay 50000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f90d6cc3fb0_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_0x5f90d6c76490;
T_5 ;
    %vpi_call 2 33 "$dumpfile", "out.vcd" {0 0 0};
    %vpi_call 2 34 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5f90d6c76490 {0 0 0};
    %delay 1000000000, 0;
    %vpi_call 2 37 "$display", "DDS test completed." {0 0 0};
    %vpi_call 2 38 "$finish" {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench.v";
    "../dds.v";
