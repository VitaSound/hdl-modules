#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x592f1855e4c0 .scope module, "testbench" "testbench" 2 3;
 .timescale -6 -9;
P_0x592f1855e650 .param/l "TEST_DURATION" 0 2 4, +C4<00000000000011110100001001000000>;
v0x592f185ac210_0 .var "clk50M", 0 0;
v0x592f185ac2e0_0 .var "key0", 0 0;
v0x592f185ac3b0_0 .net "rst", 0 0, v0x592f185abfe0_0;  1 drivers
S_0x592f1855e6f0 .scope module, "res_gen" "powerup_reset" 2 16, 3 1 0, S_0x592f1855e4c0;
 .timescale -6 -9;
    .port_info 0 /INPUT 1 "key";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /OUTPUT 1 "rst";
P_0x592f18598c00 .param/l "TICK" 0 3 2, C4<1111>;
v0x592f18598cf0_0 .net "clk", 0 0, v0x592f185ac210_0;  1 drivers
v0x592f185abf20_0 .net "key", 0 0, v0x592f185ac2e0_0;  1 drivers
v0x592f185abfe0_0 .var "rst", 0 0;
v0x592f185ac0b0_0 .var "tick_timer", 3 0;
E_0x592f185997c0 .event posedge, v0x592f18598cf0_0;
    .scope S_0x592f1855e6f0;
T_0 ;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x592f185abfe0_0, 0;
    %pushi/vec4 15, 0, 4;
    %assign/vec4 v0x592f185ac0b0_0, 0;
    %end;
    .thread T_0;
    .scope S_0x592f1855e6f0;
T_1 ;
    %wait E_0x592f185997c0;
    %load/vec4 v0x592f185abf20_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 15, 0, 4;
    %assign/vec4 v0x592f185ac0b0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x592f185abfe0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x592f185ac0b0_0;
    %cmpi/e 0, 0, 4;
    %jmp/0xz  T_1.2, 4;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x592f185abfe0_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x592f185ac0b0_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x592f185ac0b0_0, 0;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x592f1855e4c0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x592f185ac2e0_0, 0;
    %end;
    .thread T_2;
    .scope S_0x592f1855e4c0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x592f185ac210_0, 0;
    %end;
    .thread T_3;
    .scope S_0x592f1855e4c0;
T_4 ;
    %delay 10000, 0;
    %load/vec4 v0x592f185ac210_0;
    %inv;
    %assign/vec4 v0x592f185ac210_0, 0;
    %jmp T_4;
    .thread T_4;
    .scope S_0x592f1855e4c0;
T_5 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x592f185ac2e0_0, 0;
    %vpi_call 2 25 "$dumpfile", "out.vcd" {0 0 0};
    %vpi_call 2 26 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x592f1855e4c0 {0 0 0};
    %delay 1300000, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x592f185ac2e0_0, 0;
    %delay 100000, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x592f185ac2e0_0, 0;
    %delay 1000000000, 0;
    %vpi_call 2 33 "$display", "RST test completed." {0 0 0};
    %vpi_call 2 34 "$finish" {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench.v";
    "../powerup_reset.v";
